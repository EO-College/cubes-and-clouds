monthly_mean.band(\"nir\")\n\nndvi = (nir - red) / (nir + red)\n\nndvi.execute().compute()\n\ntracker.stop_experiment()","key":"sA39I8Vfkl"},{"type":"outputs","id":"es4bDe3XjSJh8nnpvXRAl","children":[],"key":"sTAu2TSKyg"}],"key":"hzr6b1PIF6"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"strong","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Quiz hint: look carefully energy emission equivalents","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"Zs7OAYCHp0"}],"key":"orMn9GTgM6"}],"key":"UVq6YgPlij"}],"key":"eVpYzFGq3b"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"calculate_emission_equivalents(experiment_id=3)","key":"lx4jtCqWjE"},{"type":"outputs","id":"WHSlrBmATTdbc3lHErJjE","children":[],"key":"vbsvJk2p4b"}],"key":"VsdWjepzMB"}],"key":"AVTGUmUYnW"},"references":{"cite":{"order":[],"data":{}}},"footer":{"navigation":{"prev":{"title":"2.4 Scaling with Pangeo","url":"/lectures/formats-and-performance/exercises/dask","group":"Data formats for achieving scalability"},"next":{"title":"3.1 Data Processing","url":"/lectures/data-processing/data-processing","group":"Process & Share"}}},"domain":"http://localhost:3000"}