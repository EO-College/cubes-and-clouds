.aws.element84.com/v1/collections/sentinel-2-l2a\"\n\nspatial_extent = {\"west\": 11.4, \"east\": 11.42, \"south\": 45.5, \"north\": 45.52}\ntemporal_extent = [\"2023-01-01\", \"2023-12-31\"]\nbands = [\"red\",\"nir\"]\n\ns2_cube = local_conn.load_stac(url=url,\n   spatial_extent=spatial_extent,\n   temporal_extent=temporal_extent,\n   bands=bands\n)\n\ns2_monthly_mean = s2_cube.aggregate_temporal_period(period=\"month\", reducer=\"mean\")\n\nred = s2_monthly_mean.band(\"red\")\nnir = s2_monthly_mean.band(\"nir\")\n\nndvi = (nir - red) / (nir + red)\n\nndvi.execute().compute()\n\ntracker.stop_experiment()","key":"P1QGQZBoaE"},{"type":"outputs","id":"VDoCIeSmjPpRzmSuCWJ_f","children":[],"key":"ZcuMZsrkHq"}],"key":"NhdZTJIFMJ"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"strong","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Quiz hint: look carefully energy emission equivalents","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"coXqeh9U17"}],"key":"IJfqovraZE"}],"key":"j6cMhtIA4r"}],"key":"ZqSHqCCF7m"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"calculate_emission_equivalents(experiment_id=3)","key":"BcmflZhnOK"},{"type":"outputs","id":"gc_nf4dFqWmgXOy0gk4j_","children":[],"key":"zUA9bbb7qc"}],"key":"meplDIVCeE"}],"key":"AXsVer4aEw"},"references":{"cite":{"order":[],"data":{}}},"footer":{"navigation":{"prev":{"title":"2.4 Scaling with Pangeo","url":"/lectures/formats-and-performance/exercises/dask","group":"Data formats for achieving scalability"},"next":{"title":"3.1 Data Processing","url":"/lectures/data-processing/data-processing","group":"Process & Share"}}},"domain":"http://localhost:3000"}