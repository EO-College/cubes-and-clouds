monthly_mean.band(\"nir\")\n\nndvi = (nir - red) / (nir + red)\n\nndvi.execute().compute()\n\ntracker.stop_experiment()","key":"iWdaGFEoGO"},{"type":"outputs","id":"WJmt5S9UtBC_EFotN3sSE","children":[],"key":"w0VoqMrJ2q"}],"key":"eK8dsmLLas"},{"type":"block","kind":"notebook-content","children":[{"type":"paragraph","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"strong","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"children":[{"type":"text","value":"Quiz hint: look carefully energy emission equivalents","position":{"start":{"line":1,"column":1},"end":{"line":1,"column":1}},"key":"LpwiOP0XuT"}],"key":"IsUNNxvUvV"}],"key":"WSNKrDDLFK"}],"key":"VVgTXmTxeH"},{"type":"block","kind":"notebook-code","children":[{"type":"code","lang":"python","executable":true,"value":"calculate_emission_equivalents(experiment_id=3)","key":"XaKKG1SEZ7"},{"type":"outputs","id":"k9XRV3Ime0zEiZTfBMmkB","children":[],"key":"reV0HSVdTP"}],"key":"XYeKHmAkaX"}],"key":"eGTb5JEklp"},"references":{"cite":{"order":[],"data":{}}},"footer":{"navigation":{"prev":{"title":"2.4 Scaling with Pangeo","url":"/lectures/formats-and-performance/exercises/dask","group":"Data formats for achieving scalability"},"next":{"title":"3.1 Data Processing","url":"/lectures/data-processing/data-processing","group":"Process & Share"}}},"domain":"http://localhost:3000"}